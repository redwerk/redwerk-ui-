{
  "name": "redwerk-ui",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/redwerk-ui.umd.js",
  "module": "./dist/redwerk-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/redwerk-ui.es.js",
      "require": "./dist/redwerk-ui.umd.js"
    }
  },
  "unpkg": "./dist/redwerk-ui.umd.js",
  "jsdelivr": "./dist/redwerk-ui.umd.js",
  "types": "./dist/types/redwerk-ui.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly --declaration",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "analyze": "vite build --config analyze.config.js",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@vueuse/core": "^9.4.0",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.13",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.22.0",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-visualizer": "^5.8.3",
    "typescript": "~4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-dts": "^1.6.6",
    "vitest": "^0.24.3",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.8"
  }
}
